/*
 * Positive: if/else
 */


if (true):
	print "Printing from true!";
else:
	print "Printing from false!";
end;

boolean b = true;

if (b):
        print "there is no else";
end;

boolean b2 = false;

if (b && b2):
        print "true && false is true?!!";
end;
if (b || b2):
        print "true || false is true!";
end;
if (b && b):
        print "true && true is true!";
end;
if (b || b):
        print "true || true is true!";
end;
if (b2 && b2):
        print "false && false is true!?";
end;
if (b2 || b2):
        print "false || false is true!?";
end;
if (! b):
        print "not true is true?!";
end;
if (! b2):
        print "not false is true!";
end;
